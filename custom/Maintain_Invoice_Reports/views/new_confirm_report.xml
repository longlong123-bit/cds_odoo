<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--Report 確認伝票-->
        <record id="paperformat_report_invoice_confirmation_new" model="report.paperformat">
            <field name="name">Invoice Confirmation Report</field>
            <field name="default" eval="False"/>
            <field name="format">A5</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">6</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">6</field>
            <field name="margin_right">6</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">0</field>
            <field name="dpi">90</field>
        </record>
        <report
            id="confirmation_reports"
            model="account.move"
            string="Invoice Confirmation"
            report_type="qweb-pdf"
            attachment_use="False"
            name="Maintain_Invoice_Reports.new_confirmation_report"
            file="Maintain_Invoice_Reports.new_confirmation_report"
            paperformat="Maintain_Invoice_Reports.paperformat_report_invoice_confirmation_new"
        />
    </data>
</odoo>
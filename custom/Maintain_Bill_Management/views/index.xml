<odoo>
    <template id="index">
        <t t-call="web.webclient_bootstrap">
            <t t-set="title" t-value="'Bill management'"/>
            <t t-set="company" t-value="company"/>
            <t t-foreach="invoices" t-as="invoice">
              <p><t t-esc="invoice"/></p>
            </t>
        </t>
    </template>
</odoo>